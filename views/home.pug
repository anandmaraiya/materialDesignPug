extends layout

block content
  include mixins
  .wrapper
    .sidebar(data-color='purple', data-image='/img/sidebar-1.jpg')
      //
        Tip 1: You can change the color of the sidebar using: data-color="purple | blue | green | orange | red"
        Tip 2: you can also add an image using data-image tag
      .logo
        a.simple-text(href='http://www.creative-tim.com')
          | Creative Tim
      .sidebar-wrapper
        ul.nav
          li.active
            a(href='/dashboard')
              i.material-icons dashboard
              p Dashboard
          li
            a(href='/charts')
              i.material-icons perm_media
              p Charts
          li
            a(href='/table')
              i.material-icons content_paste
              p Tables
          li
            a(href='/table')
              i.material-icons content_paste
              p Tables
          li
            a(href='/typography')
              i.material-icons library_books
              p Typography
          li
            a(href='/icons')
              i.material-icons bubble_chart
              p Icons
          li
            a(href='/maps')
              i.material-icons location_on
              p Maps
          li
            a(href='/notifications')
              i.material-icons.text-gray notifications
              p Notifications
          li.active-pro
            a(href='upgrade.html')
              i.material-icons unarchive
              p Upgrade to PRO
      .sidebar-background(style='background-image: url(../assets/img/sidebar-1.jpg) ')
    .main-panel
      nav.navbar.navbar-transparent.navbar-absolute
        .container-fluid
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='#') Material Dashboard
          .collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
              li
                a.dropdown-toggle(href='#pablo', data-toggle='dropdown')
                  i.material-icons dashboard
                  p.hidden-lg.hidden-md Dashboard
              li.dropdown
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  i.material-icons notifications
                  span.notification 5
                  p.hidden-lg.hidden-md Notifications
                ul.dropdown-menu
                  li
                    a(href='#') Mike John responded to your email
                  li
                    a(href='#') You have 5 new tasks
                  li
                    a(href='#') You're now friend with Andrew
                  li
                    a(href='#') Another Notification
                  li
                    a(href='#') Another One
              li
                a.dropdown-toggle(href='#pablo', data-toggle='dropdown')
                  i.material-icons person
                  p.hidden-lg.hidden-md Profile
            form.navbar-form.navbar-right(role='search')
              .form-group.is-empty
                input.form-control(type='text', placeholder='Search')
                span.material-input
                span.material-input
              button.btn.btn-white.btn-round.btn-just-icon(type='submit')
                i.material-icons search
                .ripple-container
      .content
        .container-fluid
          .row
            .col-lg-3.col-md-6.col-sm-6
              +metricCard()
            .col-lg-3.col-md-6.col-sm-6
              .card.card-stats
                .card-header(data-background-color='green')
                  i.material-icons store
                .card-content
                  p.category Revenue
                  h3.title $34,245
                .card-footer
                  .stats
                    i.material-icons date_range
                    |  Last 24 Hours
            .col-lg-3.col-md-6.col-sm-6
